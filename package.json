{
  "scripts": {
    "lint": "standard --fix preprocessor.js ./test/**/*.js",
    "prepublish": "NODE_ENV=production npm run-script tsc",
    "tsc": "tsc",
    "play": "tsc && NODE_ENV=production node -r source-map-support/register lib/playGame.js",
    "play-only": "NODE_ENV=production node -r source-map-support/register lib/playGame.js",
    "play-debug": "tsc && LOG_LEVEL=debug node --inspect -r source-map-support/register ./lib/playGame.js 2> ./play.log",
    "play-dev": "tsc && LOG_LEVEL=debug node -r source-map-support/register ./lib/playGame.js 2> ./play.log",
    "prestart": "rm -rf ./lib/; rm ./coverage/coverage-*.json; rm -rf ./coverage/lcov-report/; tsc",
    "start": "NODE_ENV=production node -r source-map-support/register ./lib/runGames.js",
    "start-dev": "tsc && LOG_LEVEL=debug node -r source-map-support/register ./lib/runGames.js 2> ./start.log",
    "start-coverage": "tsc && LOG_LEVEL=debug istanbul cover -x '**/node_modules/**' ./lib/runGames.js 2> ./start.log",
    "start-debug": "tsc && LOG_LEVEL=debug node --inspect-brk lib/runGames.js 2> ./start.log",
    "start-debug-cli": "tsc && LOG_LEVEL=debug node inspect lib/runGames.js 2> ./start.log",
    "poststart": "npm run-script coverage && echo 'Generated coverage. Type open ./coverage/lcov-html/index.html to see it'",
    "coverage": "istanbul report --root ./coverage/ lcov",
    "test": "jest --coverage --updateSnapshot @*",
    "test-debug": "node --inspect-brk -r source-map-support/register $(npm bin)/jest -i --updateSnapshot $@",
    "watch": "jest --updateSnapshot --watch",
    "debug": "tsc; node --inspect-brk lib/runGames.js"
  },
  "bin": {
    "puzzlescript": "./bin/puzzlescript.js",
    "puzzlescript-demo": "./bin/puzzlescript-demo.js"
  },
  "dependencies": {
    "@types/ansi-styles": "^3.2.0",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.108",
    "ansi-escapes": "^3.1.0",
    "ansi-styles": "^3.2.1",
    "chalk": "^2.4.1",
    "eventemitter2": "^5.0.1",
    "firstline": "^2.0.2",
    "glob": "^7.1.2",
    "inquirer": "6.0.0",
    "inquirer-autocomplete-prompt": "^0.12.2",
    "jest": "23.1.0",
    "lodash": "^4.17.10",
    "ohm-js": "^0.14.0",
    "pify": "^3.0.0",
    "source-map-support": "^0.5.6",
    "speaker": "github:philschatz/node-speaker#cac7c5b7c434b52619bfbbd9530ab0871156e815",
    "supports-color": "^5.4.0",
    "update-notifier": "^2.5.0",
    "web-audio-api": "^0.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.41",
    "@types/pify": "^3.0.2",
    "@types/supports-color": "^5.3.0",
    "eslint-plugin-typescript": "^0.12.0",
    "http-server": "^0.11.1",
    "istanbul": "^0.4.5",
    "standard": "^11.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "16.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".ts": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.+(ts|js)"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "name": "puzzlescript-cli",
  "description": "Play PuzzleScript games in your terminal!",
  "version": "1.0.7",
  "main": "lib/playGame.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philschatz/puzzlescript-cli.git"
  },
  "keywords": [
    "puzzlescript",
    "game",
    "console",
    "terminal"
  ],
  "author": "Philip Schatz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philschatz/puzzlescript-cli/issues"
  },
  "homepage": "https://github.com/philschatz/puzzlescript-cli#readme"
}
